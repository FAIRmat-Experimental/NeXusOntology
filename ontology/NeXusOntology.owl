  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXdetector_module data_size</rdfs:label>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://manual.nexusformat.org/classes/base_classes/NXdetector_module.html#nxdetector-module-data-size-field</rdfs:seeAlso>
