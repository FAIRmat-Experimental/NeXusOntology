<owl:ObjectProperty rdf:about="http://purl.org/nexusformat/definitions/NXdetector-pass_energy">
  <rdfs:range rdf:resource="#NX_ENERGY"/>
  <rdfs:subPropertyOf rdf:resource="#NeXusField"/>
